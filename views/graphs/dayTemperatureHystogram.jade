#avgDayTemperatureChart
script.
    google.charts.load('current', {packages: ['corechart', 'bar']});
    google.charts.setOnLoadCallback(drawChart);

    function drawChart() {
        var data = new google.visualization.DataTable();
        data.addColumn('number', 'Градусы');
        data.addColumn('number', 'T°C');

        data.addRows(!{JSON.stringify(dayTempHystDataset)});

        var options = {
            width: 350,
            chart: {
                title: 'Температура за последние 12 часов',
                subtitle: 'в градусах Цельсия'
            },
            hAxis: {
                title: 'К-во замеров',
                /*viewWindow: {
                    min: [7, 30],
                    max: [17, 30]
                }*/
            },
            vAxis: {
                title: 'Rating (scale of 1-10)'
            }
        };

        var material = new google.charts.Bar(document.getElementById('avgDayTemperatureChart'));
        material.draw(data, options);
    }
